fn main() -> i32 {
    let x = 10;
    let y = 20;
    let arr = [x, y, 30];
    
    // Return sum of first and last elements
    arr[0] + arr[2]  // 10 + 30 = 40
}

fn calculate(a: i32, b: i32) -> i32 {
    let result = a * b;
    result
}

fn get_array_sum() -> i32 {
    let numbers = [5, 15, 25];
    numbers[0] + numbers[1] + numbers[2]  // 5 + 15 + 25 = 45
} 