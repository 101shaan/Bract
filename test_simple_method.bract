struct Point {
    x: f64,
    y: f64,
}

impl Point {
    fn distance(&self, other: &Point) -> f64 {
        self.x + other.x
    }
}

fn main() {
    let p1 = Point { x: 3.0, y: 4.0 };
    let p2 = Point { x: 0.0, y: 0.0 };
    
    // Step 1: Test method call with simple variable
    let dist1 = p1.distance(p2);
    
    // Step 2: Test method call with address-of
    let dist2 = p1.distance(&p2);
} 